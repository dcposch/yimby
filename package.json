    "browserify": "^16.2.2",
    "node-static": "^0.7.9",
    "standard": "*",
    "uglify-js": "^3.4.9",
    "watchify": "^3.8.0"
  },
  "scripts": {
    "start": "npm run test && npm run watch & (cd static && static -z --cache false .)",
    "build": "rm -rf static/build && mkdir static/build && ./scripts/process-data.js && npm run browserify",
    "watch": "watchify -t babelify src/app.js -o static/build/bundle.js -v",
